{
  "manifest_version": 2,
  "name": "Also Known as",
  "version": "1.0",
  "description": "Portable poetic identifiers for cultural heritage objects.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": [
          "https://katalog.landesmuseum.de/object/*",
